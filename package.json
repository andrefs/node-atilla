{
  "name": "atilla",
  "version": "0.0.1",
  "description": "Browser-based GUI for ElasticSearch inspired by GenghisApp",
  "main": "index.js",
  "scripts": {
    "watch": "watchify app.js -o public/js/bundle.js -v",
    "browserify": "browserify app.js | uglifyjs > public/js/bundle.js",
    "build": "npm run browserify ",
    "start": "npm run watch & nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "elasticsearch",
    "elastic",
    "search",
    "genghis",
    "genghisapp"
  ],
  "author": "Andr√© Santos",
  "license": "ISC",
  "dependencies": {
    "async": "2.0.0-rc.2",
    "body-parser": "1.15.0",
    "cookie-parser": "1.4.1",
    "elasticsearch": "10.1.3",
    "express": "4.13.4",
    "hbs": "4.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.8"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "devDependencies": {
    "browserify": "13.0.0",
    "nodemon": "1.9.1",
    "reactify": "1.1.1",
    "uglify-js": "2.6.2",
    "watchify": "3.7.0"
  }
}
